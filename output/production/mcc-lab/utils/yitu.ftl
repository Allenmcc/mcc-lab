                    <w:p w14:paraId="2EAD8DAA" w14:textId="77777777" w:rsidP="00634254" w:rsidR="00634254" w:rsidRDefault="00634254" w:rsidRPr="0036229B">
                        <w:pPr>
                            <w:pStyle w:val="af2"/>
                            <w:numPr>
                                <w:ilvl w:val="1"/>
                                <w:numId w:val="8"/>
                            </w:numPr>
                            <w:ind w:firstLineChars="0"/>
                            <w:rPr>
                                <w:rFonts w:ascii="宋体" w:hAnsi="宋体"/>
                                <w:color w:themeColor="text1" w:val="000000"/>
                            </w:rPr>
                        </w:pPr>
                        <w:r w:rsidRPr="0036229B">
                            <w:rPr>
                                <w:rFonts w:ascii="宋体" w:hAnsi="宋体" w:hint="eastAsia"/>
                                <w:color w:themeColor="text1" w:val="000000"/>
                            </w:rPr>
                            <w:t>由于甲方</w:t>
                        </w:r>
                        <w:r w:rsidR="00EF7C5D" w:rsidRPr="0036229B">
