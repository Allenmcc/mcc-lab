                                <w:rFonts w:ascii="宋体" w:hAnsi="宋体" w:hint="eastAsia"/>
                                <w:b/>
                                <w:color w:themeColor="text1" w:val="000000"/>
                            </w:rPr>
                            <w:t>不可抗力</w:t>
                        </w:r>
                    </w:p>
                    <w:p w14:paraId="20CE8ABF" w14:textId="77777777" w:rsidP="00634254" w:rsidR="00634254" w:rsidRDefault="00634254" w:rsidRPr="0036229B">
